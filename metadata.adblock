[general]
version = 4.25.1
icons =
  icons/ab-16.png
  icons/ab-32.png
  icons/ab-48.png
  icons/ab-64.png
  icons/ab-128.png
options = options.html
browserAction =
  icons/ab-16.png
  icons/ab-19.png
  icons/ab-20.png
  icons/ab-32.png
  icons/ab-38.png
  icons/ab-40.png
